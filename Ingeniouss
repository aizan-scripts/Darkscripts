local espOn=false
local EnableESPBtn=createBtn("Enable ESP",UDim2.new(0.1,0,0.35,0),EggFrame)
local RandomizeBtn=createBtn("Randomize",UDim2.new(0.1,0,0.6,0),EggFrame)

EnableESPBtn.MouseButton1Click:Connect(function()
    print("ESP toggled")
    espOn=not espOn
    EnableESPBtn.Text=espOn and"Enabled"or"Enable ESP"
    EnableESPBtn.BackgroundColor3=espOn and Color3.fromRGB(0,170,90)or Color3.fromRGB(50,50,50)
    if espOn then
        fakeProgress(EnableESPBtn,"Enable ESP")
        applyESP()
    else
        for _,gui in ipairs(workspace:GetDescendants())do
            if gui:IsA("BillboardGui")and gui.Name=="EggESP"then gui:Destroy()end
        end
    end
end)

RandomizeBtn.MouseButton1Click:Connect(function()
    print("Randomize clicked")
    if espOn then
        fakeProgress(RandomizeBtn,"Randomize")
        randomizeESP()
    end
end)

